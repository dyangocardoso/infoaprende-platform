{
  "name": "infoaprende-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "test": "set NODE_ENV=test&& mocha --recursive --exit --file test/_setup.js",
    "test:health": "node test-health.js",
    "migrate": "npx sequelize db:migrate",
    "migrate:undo": "npx sequelize db:migrate:undo:all",
    "migrate:status": "npx sequelize db:migrate:status",
    "seed": "npx sequelize db:seed:all",
    "copy-env": "node scripts/copy-env.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "puppeteer": "^24.30.0",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.7",
    "umzug": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.0",
    "sequelize-cli": "^6.6.3",
    "sinon": "^15.2.0",
    "supertest": "^6.3.4"
  }
}
